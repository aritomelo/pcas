{
  "name": "drupol/pcas",
  "description": "pCas, a PHP CAS library",
  "type": "library",
  "license": "EUPL-1.2",
  "require": {
    "php" : "^5.6 || ^7.0 || ^7.1",
    "ext-simplexml": "*",
    "php-http/httplug": "^1.1",
    "php-http/discovery": "^1.3",
    "php-http/client-common": "^1.5",
    "symfony/security": "^3.3 || ^2.8",
    "symfony/http-foundation": "^3.3 || ^2.8",
    "symfony/serializer": "^3.3 || ^2.8",
    "symfony/yaml": "^3.3 || ^2.8",
